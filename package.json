{
	"name": "no-ceiling-sports",
	"version": "0.1.0",
	"private": true,
	"homepage": "www.noceilingsports.com/build",
	"dependencies": {
		"@alpinejs/intersect": "^3.10.4",
		"@emailjs/browser": "^3.7.0",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^13.5.0",
		"alpinejs": "^3.10.4",
		"autoprefixer": "^10.4.12",
		"postcss": "^8.4.18",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-dotenv": "^0.1.3",
		"react-scripts": "5.0.1",
		"tailwindcss": "^3.1.8",
		"web-vitals": "^2.1.4"
	},
	"scripts": {
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build",
		"start": "react-dotenv && react-scripts start",
		"build": "react-dotenv && react-scripts build",
		"test": "react-dotenv && react-scripts test",
		"eject": "react-dotenv && react-scripts eject",
		"build:css": "tailwind build -i src/style.css -o public/style.css"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"react-dotenv": {
		"whitelist": [
			"PUBLIC_KEY",
			"SERVICE_ID",
			"TEMPLATE_ID"
		]
	},
	"devDependencies": {
		"gh-pages": "^4.0.0"
	}
}
